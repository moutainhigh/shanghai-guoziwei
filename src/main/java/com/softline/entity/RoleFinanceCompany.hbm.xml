<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.softline.entity.RoleFinanceCompany" table="hh_rolefinancecompany">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native"></generator>
        </id>
        <property name="portalId" type="java.lang.Integer">
            <column name="portal_id">
                <comment></comment>
            </column>
        </property>
        <property name="roleId" type="java.lang.Integer">
            <column name="role_id">
                <comment>角色ID</comment>
            </column>
        </property>
        <property name="companyId" type="java.lang.String">
            <column name="company_id">
                <comment></comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
